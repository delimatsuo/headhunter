{
  "displayName": "Embedding Service Overview",
  "dashboardFilters": [
    {
      "filterType": "METRIC_LABEL",
      "labelKey": "environment",
      "templateVariable": "env"
    }
  ],
  "mosaicLayout": {
    "columns": 12,
    "tiles": [
      {
        "width": 6,
        "height": 4,
        "widget": {
          "title": "Embedding Latency (p50/p95/p99)",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "p50",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_embed/latency_ms\" AND metric.label.endpoint=\"/v1/embeddings/generate\" AND metric.label.percentile=\"p50\"",
                    "aggregation": {
                      "alignmentPeriod": "60s",
                      "perSeriesAligner": "ALIGN_NONE"
                    }
                  }
                }
              },
              {
                "plotType": "LINE",
                "legendTemplate": "p95",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_embed/latency_ms\" AND metric.label.endpoint=\"/v1/embeddings/generate\" AND metric.label.percentile=\"p95\"",
                    "aggregation": {
                      "alignmentPeriod": "60s",
                      "perSeriesAligner": "ALIGN_NONE"
                    }
                  }
                }
              },
              {
                "plotType": "LINE",
                "legendTemplate": "p99",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_embed/latency_ms\" AND metric.label.endpoint=\"/v1/embeddings/generate\" AND metric.label.percentile=\"p99\"",
                    "aggregation": {
                      "alignmentPeriod": "60s",
                      "perSeriesAligner": "ALIGN_NONE"
                    }
                  }
                }
              }
            ],
            "yAxis": {
              "label": "Milliseconds"
            }
          }
        }
      },
      {
        "width": 6,
        "height": 4,
        "widget": {
          "title": "Embedding Throughput",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "STACKED_BAR",
                "legendTemplate": "generate",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_embed/request_count\" AND metric.label.endpoint=\"/v1/embeddings/generate\" AND metric.label.result=\"success\"",
                    "aggregation": {
                      "alignmentPeriod": "60s",
                      "perSeriesAligner": "ALIGN_RATE"
                    }
                  }
                }
              },
              {
                "plotType": "STACKED_BAR",
                "legendTemplate": "upsert",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_embed/request_count\" AND metric.label.endpoint=\"/v1/embeddings/upsert\" AND metric.label.result=\"success\"",
                    "aggregation": {
                      "alignmentPeriod": "60s",
                      "perSeriesAligner": "ALIGN_RATE"
                    }
                  }
                }
              },
              {
                "plotType": "STACKED_BAR",
                "legendTemplate": "query",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_embed/request_count\" AND metric.label.endpoint=\"/v1/embeddings/query\" AND metric.label.result=\"success\"",
                    "aggregation": {
                      "alignmentPeriod": "60s",
                      "perSeriesAligner": "ALIGN_RATE"
                    }
                  }
                }
              }
            ],
            "yAxis": {
              "label": "Requests/min"
            }
          }
        }
      },
      {
        "width": 6,
        "height": 4,
        "widget": {
          "title": "Error Rate by Endpoint",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "generate",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_embed/error_ratio\" AND metric.label.endpoint=\"/v1/embeddings/generate\"",
                    "aggregation": {
                      "alignmentPeriod": "120s",
                      "perSeriesAligner": "ALIGN_MEAN"
                    }
                  }
                }
              },
              {
                "plotType": "LINE",
                "legendTemplate": "upsert",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_embed/error_ratio\" AND metric.label.endpoint=\"/v1/embeddings/upsert\"",
                    "aggregation": {
                      "alignmentPeriod": "120s",
                      "perSeriesAligner": "ALIGN_MEAN"
                    }
                  }
                }
              },
              {
                "plotType": "LINE",
                "legendTemplate": "query",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_embed/error_ratio\" AND metric.label.endpoint=\"/v1/embeddings/query\"",
                    "aggregation": {
                      "alignmentPeriod": "120s",
                      "perSeriesAligner": "ALIGN_MEAN"
                    }
                  }
                }
              }
            ]
          }
        }
      },
      {
        "width": 6,
        "height": 4,
        "widget": {
          "title": "Gemini Integration Health",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "success rate",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_embed/gemini_success_ratio\"",
                    "aggregation": {
                      "alignmentPeriod": "120s",
                      "perSeriesAligner": "ALIGN_MEAN"
                    }
                  }
                }
              },
              {
                "plotType": "LINE",
                "legendTemplate": "latency p95",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_embed/gemini_latency_ms\" AND metric.label.percentile=\"p95\"",
                    "aggregation": {
                      "alignmentPeriod": "120s",
                      "perSeriesAligner": "ALIGN_NONE"
                    }
                  }
                }
              }
            ]
          }
        }
      },
      {
        "width": 6,
        "height": 4,
        "widget": {
          "title": "pgvector Upsert Performance",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "latency p95",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_embed/pgvector_upsert_latency_ms\" AND metric.label.percentile=\"p95\"",
                    "aggregation": {
                      "alignmentPeriod": "120s",
                      "perSeriesAligner": "ALIGN_NONE"
                    }
                  }
                }
              },
              {
                "plotType": "LINE",
                "legendTemplate": "error rate",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_embed/pgvector_upsert_error_ratio\"",
                    "aggregation": {
                      "alignmentPeriod": "120s",
                      "perSeriesAligner": "ALIGN_MEAN"
                    }
                  }
                }
              }
            ]
          }
        }
      },
      {
        "width": 6,
        "height": 4,
        "widget": {
          "title": "Cache Hit Rate",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "embedding cache",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_embed/cache_hit_ratio\"",
                    "aggregation": {
                      "alignmentPeriod": "300s",
                      "perSeriesAligner": "ALIGN_MEAN"
                    }
                  }
                }
              }
            ]
          }
        }
      },
      {
        "width": 6,
        "height": 4,
        "widget": {
          "title": "Tenant Utilization",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "STACKED_BAR",
                "legendTemplate": "{{metric.label.tenant}}",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_embed/tenant_embedding_requests\"",
                    "aggregation": {
                      "alignmentPeriod": "300s",
                      "perSeriesAligner": "ALIGN_RATE"
                    }
                  }
                }
              }
            ],
            "yAxis": {
              "label": "Requests/min"
            }
          }
        }
      },
      {
        "width": 12,
        "height": 4,
        "widget": {
          "title": "Recent Embedding Errors",
          "logPanel": {
            "filter": "resource.type=\"cloud_run_revision\" AND resource.labels.service_name=\"hh-embed-svc\" AND severity>=ERROR",
            "tableDisplayOptions": {
              "shownColumns": [
                "timestamp",
                "severity",
                "text"
              ]
            }
          }
        }
      }
    ]
  }
}
