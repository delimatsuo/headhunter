{
  "displayName": "ECO Service Overview",
  "dashboardFilters": [
    {
      "filterType": "METRIC_LABEL",
      "labelKey": "environment",
      "templateVariable": "env"
    }
  ],
  "mosaicLayout": {
    "columns": 12,
    "tiles": [
      {
        "width": 6,
        "height": 4,
        "widget": {
          "title": "Occupation Search Latency",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "p50",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_eco/search_latency_ms\" AND metric.label.percentile=\"p50\"",
                    "aggregation": {
                      "alignmentPeriod": "60s",
                      "perSeriesAligner": "ALIGN_NONE"
                    }
                  }
                }
              },
              {
                "plotType": "LINE",
                "legendTemplate": "p95",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_eco/search_latency_ms\" AND metric.label.percentile=\"p95\"",
                    "aggregation": {
                      "alignmentPeriod": "60s",
                      "perSeriesAligner": "ALIGN_NONE"
                    }
                  }
                }
              },
              {
                "plotType": "LINE",
                "legendTemplate": "p99",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_eco/search_latency_ms\" AND metric.label.percentile=\"p99\"",
                    "aggregation": {
                      "alignmentPeriod": "60s",
                      "perSeriesAligner": "ALIGN_NONE"
                    }
                  }
                }
              }
            ],
            "yAxis": {
              "label": "Milliseconds"
            }
          }
        }
      },
      {
        "width": 6,
        "height": 4,
        "widget": {
          "title": "Search Throughput",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "STACKED_BAR",
                "legendTemplate": "search",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_eco/request_count\" AND metric.label.endpoint=\"/v1/occupations/search\" AND metric.label.result=\"success\"",
                    "aggregation": {
                      "alignmentPeriod": "60s",
                      "perSeriesAligner": "ALIGN_RATE"
                    }
                  }
                }
              },
              {
                "plotType": "STACKED_BAR",
                "legendTemplate": "fetch",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_eco/request_count\" AND metric.label.endpoint=\"/v1/occupations/{eco_id}\" AND metric.label.result=\"success\"",
                    "aggregation": {
                      "alignmentPeriod": "60s",
                      "perSeriesAligner": "ALIGN_RATE"
                    }
                  }
                }
              }
            ],
            "yAxis": {
              "label": "Requests/min"
            }
          }
        }
      },
      {
        "width": 6,
        "height": 4,
        "widget": {
          "title": "Fuzzy Matching Performance",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "match score",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_eco/fuzzy_match_score\"",
                    "aggregation": {
                      "alignmentPeriod": "120s",
                      "perSeriesAligner": "ALIGN_MEAN"
                    }
                  }
                }
              },
              {
                "plotType": "LINE",
                "legendTemplate": "fallback ratio",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_eco/fallback_ratio\"",
                    "aggregation": {
                      "alignmentPeriod": "120s",
                      "perSeriesAligner": "ALIGN_MEAN"
                    }
                  }
                }
              }
            ]
          }
        }
      },
      {
        "width": 6,
        "height": 4,
        "widget": {
          "title": "Cache Hit Rate",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "hit ratio",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_eco/cache_hit_ratio\"",
                    "aggregation": {
                      "alignmentPeriod": "300s",
                      "perSeriesAligner": "ALIGN_MEAN"
                    }
                  }
                }
              }
            ]
          }
        }
      },
      {
        "width": 6,
        "height": 4,
        "widget": {
          "title": "Portuguese Normalization",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "success ratio",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_eco/normalization_success_ratio\"",
                    "aggregation": {
                      "alignmentPeriod": "120s",
                      "perSeriesAligner": "ALIGN_MEAN"
                    }
                  }
                }
              },
              {
                "plotType": "LINE",
                "legendTemplate": "fallback count",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_eco/normalization_fallback_count\"",
                    "aggregation": {
                      "alignmentPeriod": "120s",
                      "perSeriesAligner": "ALIGN_RATE"
                    }
                  }
                }
              }
            ]
          }
        }
      },
      {
        "width": 6,
        "height": 4,
        "widget": {
          "title": "Tenant Utilization",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "STACKED_BAR",
                "legendTemplate": "{{metric.label.tenant}}",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_eco/tenant_request_count\"",
                    "aggregation": {
                      "alignmentPeriod": "300s",
                      "perSeriesAligner": "ALIGN_RATE"
                    }
                  }
                }
              }
            ],
            "yAxis": {
              "label": "Requests/min"
            }
          }
        }
      },
      {
        "width": 12,
        "height": 4,
        "widget": {
          "title": "Recent ECO Errors",
          "logPanel": {
            "filter": "resource.type=\"cloud_run_revision\" AND resource.labels.service_name=\"hh-eco-svc\" AND severity>=ERROR",
            "tableDisplayOptions": {
              "shownColumns": [
                "timestamp",
                "severity",
                "text"
              ]
            }
          }
        }
      }
    ]
  }
}
