{
  "displayName": "Search Service Overview",
  "dashboardFilters": [
    {
      "filterType": "METRIC_LABEL",
      "labelKey": "environment",
      "templateVariable": "env"
    }
  ],
  "mosaicLayout": {
    "columns": 12,
    "tiles": [
      {
        "width": 6,
        "height": 4,
        "widget": {
          "title": "Hybrid Search Latency (p50/p95/p99)",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "p50",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_search/hybrid_latency_ms\" AND metric.label.percentile=\"p50\"",
                    "aggregation": {
                      "alignmentPeriod": "60s",
                      "perSeriesAligner": "ALIGN_NONE"
                    }
                  }
                }
              },
              {
                "plotType": "LINE",
                "legendTemplate": "p95",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_search/hybrid_latency_ms\" AND metric.label.percentile=\"p95\"",
                    "aggregation": {
                      "alignmentPeriod": "60s",
                      "perSeriesAligner": "ALIGN_NONE"
                    }
                  }
                }
              },
              {
                "plotType": "LINE",
                "legendTemplate": "p99",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_search/hybrid_latency_ms\" AND metric.label.percentile=\"p99\"",
                    "aggregation": {
                      "alignmentPeriod": "60s",
                      "perSeriesAligner": "ALIGN_NONE"
                    }
                  }
                }
              }
            ],
            "yAxis": {
              "label": "Milliseconds"
            }
          }
        }
      },
      {
        "width": 6,
        "height": 4,
        "widget": {
          "title": "Search Throughput",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "STACKED_BAR",
                "legendTemplate": "hybrid",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_search/request_count\" AND metric.label.endpoint=\"/v1/search/hybrid\" AND metric.label.result=\"success\"",
                    "aggregation": {
                      "alignmentPeriod": "60s",
                      "perSeriesAligner": "ALIGN_RATE"
                    }
                  }
                }
              }
            ],
            "yAxis": {
              "label": "Searches/min"
            }
          }
        }
      },
      {
        "width": 6,
        "height": 4,
        "widget": {
          "title": "Cache Hit Rate",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "hit rate",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_search/cache_hit_ratio\"",
                    "aggregation": {
                      "alignmentPeriod": "300s",
                      "perSeriesAligner": "ALIGN_MEAN"
                    }
                  }
                }
              }
            ]
          }
        }
      },
      {
        "width": 6,
        "height": 4,
        "widget": {
          "title": "FTS vs Vector Latency",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "fts p95",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_search/fts_latency_ms\" AND metric.label.percentile=\"p95\"",
                    "aggregation": {
                      "alignmentPeriod": "120s",
                      "perSeriesAligner": "ALIGN_NONE"
                    }
                  }
                }
              },
              {
                "plotType": "LINE",
                "legendTemplate": "vector p95",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_search/vector_latency_ms\" AND metric.label.percentile=\"p95\"",
                    "aggregation": {
                      "alignmentPeriod": "120s",
                      "perSeriesAligner": "ALIGN_NONE"
                    }
                  }
                }
              }
            ]
          }
        }
      },
      {
        "width": 6,
        "height": 4,
        "widget": {
          "title": "Redis Integration Health",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "latency p95",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_search/redis_latency_ms\" AND metric.label.percentile=\"p95\"",
                    "aggregation": {
                      "alignmentPeriod": "120s",
                      "perSeriesAligner": "ALIGN_NONE"
                    }
                  }
                }
              },
              {
                "plotType": "LINE",
                "legendTemplate": "error ratio",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_search/redis_error_ratio\"",
                    "aggregation": {
                      "alignmentPeriod": "120s",
                      "perSeriesAligner": "ALIGN_NONE"
                    }
                  }
                }
              }
            ]
          }
        }
      },
      {
        "width": 6,
        "height": 4,
        "widget": {
          "title": "pgvector Query Performance",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "latency p95",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_search/pgvector_query_latency_ms\" AND metric.label.percentile=\"p95\"",
                    "aggregation": {
                      "alignmentPeriod": "120s",
                      "perSeriesAligner": "ALIGN_NONE"
                    }
                  }
                }
              },
              {
                "plotType": "LINE",
                "legendTemplate": "error ratio",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_search/pgvector_query_error_ratio\"",
                    "aggregation": {
                      "alignmentPeriod": "120s",
                      "perSeriesAligner": "ALIGN_MEAN"
                    }
                  }
                }
              }
            ]
          }
        }
      },
      {
        "width": 6,
        "height": 4,
        "widget": {
          "title": "Service Dependency Health",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "embed success",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_search/embed_dependency_success_ratio\"",
                    "aggregation": {
                      "alignmentPeriod": "180s",
                      "perSeriesAligner": "ALIGN_MEAN"
                    }
                  }
                }
              },
              {
                "plotType": "LINE",
                "legendTemplate": "rerank success",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_search/rerank_dependency_success_ratio\"",
                    "aggregation": {
                      "alignmentPeriod": "180s",
                      "perSeriesAligner": "ALIGN_MEAN"
                    }
                  }
                }
              }
            ]
          }
        }
      },
      {
        "width": 6,
        "height": 4,
        "widget": {
          "title": "Tenant Utilization",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "STACKED_BAR",
                "legendTemplate": "{{metric.label.tenant}}",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_search/tenant_search_requests\"",
                    "aggregation": {
                      "alignmentPeriod": "300s",
                      "perSeriesAligner": "ALIGN_RATE"
                    }
                  }
                }
              }
            ],
            "yAxis": {
              "label": "Searches/min"
            }
          }
        }
      },
      {
        "width": 12,
        "height": 4,
        "widget": {
          "title": "Recent Search Errors",
          "logPanel": {
            "filter": "resource.type=\"cloud_run_revision\" AND resource.labels.service_name=\"hh-search-svc\" AND severity>=ERROR",
            "tableDisplayOptions": {
              "shownColumns": [
                "timestamp",
                "severity",
                "text"
              ]
            }
          }
        }
      }
    ]
  }
}
