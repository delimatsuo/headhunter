{
  "displayName": "Enrichment Service Overview",
  "dashboardFilters": [],
  "mosaicLayout": {
    "columns": 12,
    "tiles": [
      {
        "width": 6,
        "height": 4,
        "widget": {
          "title": "Job Latency (p50/p95/p99)",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "p50",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_enrich/job_latency_ms\" AND metric.label.percentile=\"p50\"",
                    "aggregation": {
                      "alignmentPeriod": "60s",
                      "perSeriesAligner": "ALIGN_MEAN"
                    }
                  }
                }
              },
              {
                "plotType": "LINE",
                "legendTemplate": "p95",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_enrich/job_latency_ms\" AND metric.label.percentile=\"p95\"",
                    "aggregation": {
                      "alignmentPeriod": "60s",
                      "perSeriesAligner": "ALIGN_MEAN"
                    }
                  }
                }
              },
              {
                "plotType": "LINE",
                "legendTemplate": "p99",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_enrich/job_latency_ms\" AND metric.label.percentile=\"p99\"",
                    "aggregation": {
                      "alignmentPeriod": "60s",
                      "perSeriesAligner": "ALIGN_MEAN"
                    }
                  }
                }
              }
            ],
            "yAxis": {
              "label": "Milliseconds"
            }
          }
        }
      },
      {
        "width": 6,
        "height": 4,
        "widget": {
          "title": "Job Throughput & Failures",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "STACKED_BAR",
                "legendTemplate": "completed",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_enrich/job_completed_count\"",
                    "aggregation": {
                      "alignmentPeriod": "60s",
                      "perSeriesAligner": "ALIGN_RATE"
                    }
                  }
                }
              },
              {
                "plotType": "STACKED_BAR",
                "legendTemplate": "failed",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_enrich/job_failed_count\"",
                    "aggregation": {
                      "alignmentPeriod": "60s",
                      "perSeriesAligner": "ALIGN_RATE"
                    }
                  }
                }
              }
            ],
            "yAxis": {
              "label": "Jobs/min"
            }
          }
        }
      },
      {
        "width": 6,
        "height": 4,
        "widget": {
          "title": "Queue Depth",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "queued jobs",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_enrich/queue_depth\"",
                    "aggregation": {
                      "alignmentPeriod": "60s",
                      "perSeriesAligner": "ALIGN_MEAN"
                    }
                  }
                }
              }
            ]
          }
        }
      },
      {
        "width": 6,
        "height": 4,
        "widget": {
          "title": "Embedding Service Health",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "success rate",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_enrich/embed_success_ratio\"",
                    "aggregation": {
                      "alignmentPeriod": "60s",
                      "perSeriesAligner": "ALIGN_MEAN"
                    }
                  }
                }
              },
              {
                "plotType": "LINE",
                "legendTemplate": "circuit state",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_enrich/embed_circuit_state\"",
                    "aggregation": {
                      "alignmentPeriod": "60s",
                      "perSeriesAligner": "ALIGN_MEAN"
                    }
                  }
                }
              }
            ]
          }
        }
      },
      {
        "width": 12,
        "height": 4,
        "widget": {
          "title": "Tenant Utilization",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "STACKED_BAR",
                "legendTemplate": "{{metric.label.tenant}}",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_enrich/tenant_job_count\"",
                    "aggregation": {
                      "alignmentPeriod": "300s",
                      "perSeriesAligner": "ALIGN_RATE"
                    }
                  }
                }
              }
            ],
            "yAxis": {
              "label": "Jobs/min"
            }
          }
        }
      },
      {
        "width": 12,
        "height": 4,
        "widget": {
          "title": "Recent Job Failures (Log Panel)",
          "logPanel": {
            "filter": "resource.type=\"cloud_run_revision\" AND resource.labels.service_name=\"hh-enrich-svc\" AND severity>=ERROR",
            "tableDisplayOptions": {
              "shownColumns": ["timestamp", "severity", "text"]
            }
          }
        }
      }
    ]
  }
}
