{
  "displayName": "MSGS Service Overview",
  "dashboardFilters": [],
  "mosaicLayout": {
    "columns": 12,
    "tiles": [
      {
        "width": 6,
        "height": 4,
        "widget": {
          "title": "Skill Expansion Request Rate",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "requests/min",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_msgs/skills_expand_requests\"",
                    "aggregation": {
                      "alignmentPeriod": "300s",
                      "perSeriesAligner": "ALIGN_RATE"
                    }
                  }
                }
              },
              {
                "plotType": "LINE",
                "legendTemplate": "cache hit %",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_msgs/skills_cache_hit_ratio\"",
                    "aggregation": {
                      "alignmentPeriod": "300s",
                      "perSeriesAligner": "ALIGN_MEAN"
                    }
                  },
                  "unitOverride": "ratio"
                },
                "targetAxis": "Y2"
              }
            ],
            "yAxis": {
              "label": "Requests"
            },
            "y2Axis": {
              "label": "Ratio"
            }
          }
        }
      },
      {
        "width": 6,
        "height": 4,
        "widget": {
          "title": "Role Template Latency (p95)",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "latency",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_msgs/roles_template_latency_ms\"",
                    "aggregation": {
                      "alignmentPeriod": "300s",
                      "perSeriesAligner": "ALIGN_PERCENTILE_95"
                    }
                  }
                }
              }
            ],
            "yAxis": {
              "label": "Milliseconds"
            }
          }
        }
      },
      {
        "width": 6,
        "height": 4,
        "widget": {
          "title": "Market Demand Computation Latency",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "EMA compute",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_msgs/demand_compute_latency_ms\" AND metric.label.stage=\"ema\"",
                    "aggregation": {
                      "alignmentPeriod": "300s",
                      "perSeriesAligner": "ALIGN_PERCENTILE_95"
                    }
                  }
                }
              },
              {
                "plotType": "LINE",
                "legendTemplate": "Z-score",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_msgs/demand_compute_latency_ms\" AND metric.label.stage=\"zscore\"",
                    "aggregation": {
                      "alignmentPeriod": "300s",
                      "perSeriesAligner": "ALIGN_PERCENTILE_95"
                    }
                  }
                }
              }
            ],
            "yAxis": {
              "label": "Milliseconds"
            }
          }
        }
      },
      {
        "width": 6,
        "height": 4,
        "widget": {
          "title": "Redis Cache Health",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "latency",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_msgs/redis_ping_latency_ms\"",
                    "aggregation": {
                      "alignmentPeriod": "300s",
                      "perSeriesAligner": "ALIGN_MEAN"
                    }
                  }
                }
              }
            ],
            "yAxis": {
              "label": "Milliseconds"
            }
          }
        }
      },
      {
        "width": 12,
        "height": 4,
        "widget": {
          "title": "Cloud SQL Connections",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "in use",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"cloudsql.googleapis.com/database/network/connections\" AND resource.label.database_id=monitoring.regex.full_match(\".*msgs.*\")",
                    "aggregation": {
                      "alignmentPeriod": "300s",
                      "perSeriesAligner": "ALIGN_MEAN"
                    }
                  }
                }
              }
            ],
            "yAxis": {
              "label": "Connections"
            }
          }
        }
      },
      {
        "width": 12,
        "height": 4,
        "widget": {
          "title": "Recent MSGS Errors",
          "logPanel": {
            "filter": "resource.type=\"cloud_run_revision\" AND resource.labels.service_name=\"hh-msgs-svc\" AND severity>=ERROR",
            "tableDisplayOptions": {
              "shownColumns": ["timestamp", "severity", "text"]
            }
          }
        }
      }
    ]
  }
}
