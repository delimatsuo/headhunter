{
  "displayName": "Admin Service Overview",
  "dashboardFilters": [],
  "mosaicLayout": {
    "columns": 12,
    "tiles": [
      {
        "width": 6,
        "height": 4,
        "widget": {
          "title": "Refresh Job Success vs Failure",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "STACKED_BAR",
                "legendTemplate": "success",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_admin/refresh_job_success\"",
                    "aggregation": {
                      "alignmentPeriod": "300s",
                      "perSeriesAligner": "ALIGN_RATE"
                    }
                  }
                }
              },
              {
                "plotType": "STACKED_BAR",
                "legendTemplate": "failure",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_admin/refresh_job_failure\"",
                    "aggregation": {
                      "alignmentPeriod": "300s",
                      "perSeriesAligner": "ALIGN_RATE"
                    }
                  }
                }
              }
            ],
            "yAxis": {
              "label": "Jobs/min"
            }
          }
        }
      },
      {
        "width": 6,
        "height": 4,
        "widget": {
          "title": "Refresh Latency",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "postings",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_admin/refresh_latency_ms\" AND metric.label.scope=\"postings\"",
                    "aggregation": {
                      "alignmentPeriod": "300s",
                      "perSeriesAligner": "ALIGN_PERCENTILE_95"
                    }
                  }
                }
              },
              {
                "plotType": "LINE",
                "legendTemplate": "profiles",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_admin/refresh_latency_ms\" AND metric.label.scope=\"profiles\"",
                    "aggregation": {
                      "alignmentPeriod": "300s",
                      "perSeriesAligner": "ALIGN_PERCENTILE_95"
                    }
                  }
                }
              }
            ],
            "yAxis": {
              "label": "Milliseconds"
            }
          }
        }
      },
      {
        "width": 6,
        "height": 4,
        "widget": {
          "title": "Posting Freshness Lag",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "max lag",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_admin/postings_max_lag_days\"",
                    "aggregation": {
                      "alignmentPeriod": "600s",
                      "perSeriesAligner": "ALIGN_MAX"
                    }
                  }
                }
              },
              {
                "plotType": "LINE",
                "legendTemplate": "stale tenants",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_admin/postings_stale_tenant_count\"",
                    "aggregation": {
                      "alignmentPeriod": "600s",
                      "perSeriesAligner": "ALIGN_MEAN"
                    }
                  }
                }
              }
            ],
            "yAxis": {
              "label": "Days"
            }
          }
        }
      },
      {
        "width": 6,
        "height": 4,
        "widget": {
          "title": "Profile Freshness Lag",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "max lag",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_admin/profiles_max_lag_days\"",
                    "aggregation": {
                      "alignmentPeriod": "600s",
                      "perSeriesAligner": "ALIGN_MAX"
                    }
                  }
                }
              },
              {
                "plotType": "LINE",
                "legendTemplate": "stale tenants",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_admin/profiles_stale_tenant_count\"",
                    "aggregation": {
                      "alignmentPeriod": "600s",
                      "perSeriesAligner": "ALIGN_MEAN"
                    }
                  }
                }
              }
            ],
            "yAxis": {
              "label": "Days"
            }
          }
        }
      },
      {
        "width": 12,
        "height": 4,
        "widget": {
          "title": "Scheduler Run Status",
          "xyChart": {
            "dataSets": [
              {
                "plotType": "STACKED_BAR",
                "legendTemplate": "{{metric.label.job}}",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/hh_admin/scheduler_run_count\"",
                    "aggregation": {
                      "alignmentPeriod": "900s",
                      "perSeriesAligner": "ALIGN_RATE"
                    }
                  }
                }
              }
            ],
            "yAxis": {
              "label": "Runs/min"
            }
          }
        }
      },
      {
        "width": 12,
        "height": 4,
        "widget": {
          "title": "Recent Admin Errors",
          "logPanel": {
            "filter": "resource.type=\"cloud_run_revision\" AND resource.labels.service_name=\"hh-admin-svc\" AND severity>=ERROR",
            "tableDisplayOptions": {
              "shownColumns": ["timestamp", "severity", "text"]
            }
          }
        }
      }
    ]
  }
}
